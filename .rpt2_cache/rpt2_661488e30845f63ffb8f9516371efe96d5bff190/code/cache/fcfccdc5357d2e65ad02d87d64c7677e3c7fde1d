  "code": "import Snake from \"./Snake\";\r\nexport default class RunningGame {\r\n    runGame() {\r\n        var snake = [new Snake];\r\n        Laya.stage.addChild(snake[0]);\r\n        snake[0].graphics.drawRect(0, 0, 10, 10, \"#ffff20\");\r\n        var food = new Laya.Sprite();\r\n        var xCoordinate;\r\n        var yCoordinate;\r\n        var arrCoordinate = new Array(40 * 40);\r\n        Laya.stage.addChild(food);\r\n        food.graphics.drawRect(0, 0, 10, 10, \"#ffff20\");\r\n        for (var x = 0; x < 40; x++) {\r\n            arrCoordinate[x] = [(x * 100) + 0, (x * 100) + 1, (x * 100) + 2, (x * 100) + 3, (x * 100) + 4, (x * 100) + 5, (x * 100) + 6,\r\n                (x * 100) + 7, (x * 100) + 8, (x * 100) + 9, (x * 100) + 10, (x * 100) + 11, (x * 100) + 12, (x * 100) + 13, (x * 100) + 14,\r\n                (x * 100) + 15, (x * 100) + 16, (x * 100) + 17, (x * 100) + 18, (x * 100) + 19, (x * 100) + 20, (x * 100) + 21, (x * 100) + 22,\r\n                (x * 100) + 23, (x * 100) + 24, (x * 100) + 25, (x * 100) + 26, (x * 100) + 27, (x * 100) + 28, (x * 100) + 29, (x * 100) + 30,\r\n                (x * 100) + 31, (x * 100) + 32, (x * 100) + 33, (x * 100) + 34, (x * 100) + 35, (x * 100) + 36, (x * 100) + 37, (x * 100) + 38,\r\n                (x * 100) + 39];\r\n        }\r\n        function pushArr() {\r\n            var x = Math.floor(Math.random() * 39);\r\n            var y = Math.floor(Math.random() * 39);\r\n            var temp = arrCoordinate[x][y];\r\n            xCoordinate = Math.floor(temp / 100) * 10;\r\n            for (temp; true; temp -= 100) {\r\n                if (temp - 100 < 0) {\r\n                    yCoordinate = temp * 10;\r\n                    break;\r\n                }\r\n            }\r\n            food.x = xCoordinate;\r\n            food.y = yCoordinate;\r\n        }\r\n        pushArr();\r\n        function onKeyDown(e) {\r\n            var keyCode = e[\"keyCode\"];\r\n            if (keyCode == 37 && snake[0].fangxiang != 4)\r\n                snake[0].fangxiang = 3;\r\n            if (keyCode == 38 && snake[0].fangxiang != 2)\r\n                snake[0].fangxiang = 1;\r\n            if (keyCode == 39 && snake[0].fangxiang != 3)\r\n                snake[0].fangxiang = 4;\r\n            if (keyCode == 40 && snake[0].fangxiang != 1)\r\n                snake[0].fangxiang = 2;\r\n        }\r\n        function run() {\r\n            var xOld = snake[0].x;\r\n            var yOld = snake[0].y;\r\n            var fOld = snake[0].fangxiang;\r\n            for (var i = 1; i < snake.length; i++) {\r\n                var xTemp = snake[i].x;\r\n                var yTemp = snake[i].y;\r\n                var fTemp = snake[i].fangxiang;\r\n                snake[i].x = xOld;\r\n                snake[i].y = yOld;\r\n                snake[i].fangxiang = fOld;\r\n                xOld = xTemp;\r\n                yOld = yTemp;\r\n                fOld = fTemp;\r\n            }\r\n            if (snake[0].fangxiang == 1)\r\n                snake[0].y -= 10;\r\n            if (snake[0].fangxiang == 2)\r\n                snake[0].y += 10;\r\n            if (snake[0].fangxiang == 3)\r\n                snake[0].x -= 10;\r\n            if (!snake[0].fangxiang || snake[0].fangxiang == 4)\r\n                snake[0].x += 10;\r\n        }\r\n        function eat() {\r\n            if (snake[0].x == xCoordinate && snake[0].y == yCoordinate) {\r\n                snake[0].jie += 1;\r\n                snake[snake[0].jie] = new Snake();\r\n                var temp = snake[0].jie;\r\n                var xTemp = snake[snake[0].jie - 1].x;\r\n                var yTemp = snake[snake[0].jie - 1].y;\r\n                snake[temp].graphics.drawRect(0, 0, 10, 10, \"#ffff20\");\r\n                if (snake[temp - 1].fangxiang == 1) {\r\n                    snake[temp].x = xTemp;\r\n                    snake[temp].y = yTemp + 10;\r\n                    snake[temp].fangxiang = snake[temp - 1].fangxiang;\r\n                }\r\n                if (snake[temp - 1].fangxiang == 2) {\r\n                    snake[temp].x = xTemp;\r\n                    snake[temp].y = yTemp - 10;\r\n                    snake[temp].fangxiang = snake[temp - 1].fangxiang;\r\n                }\r\n                if (snake[temp - 1].fangxiang == 3) {\r\n                    snake[temp].x = xTemp + 10;\r\n                    snake[temp].y = yTemp;\r\n                    snake[temp].fangxiang = snake[temp - 1].fangxiang;\r\n                }\r\n                if (snake[temp - 1].fangxiang == 4) {\r\n                    snake[temp].x = xTemp - 10;\r\n                    snake[temp].y = yTemp;\r\n                    snake[temp].fangxiang = snake[temp - 1].fangxiang;\r\n                }\r\n                Laya.stage.addChild(snake[temp]);\r\n                pushArr();\r\n            }\r\n        }\r\n        function isOver() {\r\n            var gameOver = false;\r\n            if (snake[0].x < 0 || snake[0].y < 0 || snake[0].y > 390 || snake[0].x > 390)\r\n                gameOver = true;\r\n            for (var i = 1; i < snake.length; i++) {\r\n                if (snake[0].x == snake[i].x && snake[0].y == snake[i].y)\r\n                    gameOver = true;\r\n            }\r\n            if (gameOver) {\r\n                clearInterval(tRun);\r\n                clearInterval(tIsOver);\r\n                clearInterval(tEat);\r\n                alert(\"游戏结束\");\r\n                Laya.Scene.open(\"gameRestart.scene\");\r\n            }\r\n        }\r\n        Laya.stage.on(Laya.Event.KEY_DOWN, null, onKeyDown);\r\n        var tEat = setInterval(eat, 10);\r\n        var tRun = setInterval(run, 200);\r\n        var tIsOver = setInterval(isOver, 200);\r\n    }\r\n}\r\n//# sourceMappingURL=RunningGame.js.map",
